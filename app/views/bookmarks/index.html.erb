<p><%= link_to 'New Bookmark', new_bookmark_path %></p>

<dl>
	<% @bookmarks.each do |bookmark| %>
		<dt><% if bookmark.favorite == true %></dt>
				<strong><%= link_to bookmark.title, bookmark %></strong>
				<% else %>
					<%= link_to bookmark.title, bookmark %>
				<% end %>
		<dd>Url: <%= link_to bookmark.url, bookmark %></dd>
		<dd>Comments: <%= link_to bookmark.comment, bookmark %></dd>
		<dd>On your favorites list? <%= link_to bookmark.favorite, bookmark %></dd>
	<% end %>
</dl>


