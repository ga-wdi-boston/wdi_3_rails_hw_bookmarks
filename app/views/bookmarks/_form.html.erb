<%= form_for @bookmark do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :url %>
    <%= form.text_field :url%>
  </div>
    <div>
    <%= form.label :comment %>
    <%= form.text_field :comment%>
  </div>
    <div>
    <%= form.label :favorite %>
    <%= form.text_field :favorite%>
  </div>
  <%= form.submit %>
<% end %>
