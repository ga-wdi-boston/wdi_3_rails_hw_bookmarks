<%= form_for @bookmark do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>
  <div>
    <%= f.label :comment %>
    <%= f.text_area :comment %>
  </div>
  <div>
    <%= f.label :category %>
    <%= f.select :category, Bookmark::CATEGORIES %>
  </div>
  <div>
    <%= f.label :is_fav, 'Favorite?' %>
    <%= f.check_box :is_fav %>
  </div>
  <%= f.submit %>
<% end %>
