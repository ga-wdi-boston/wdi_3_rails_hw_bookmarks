<%= form_for bookmark do |f| %>
    <%= f.label :url %> <br>
    <%= f.text_field :url %>
    <br>
    <%= f.label :title %> <br>
    <%= f.text_area :title %>
    <br>
    <%= f.label :category %> <br>
    <%= f.text_field :category %>
    <br>
    <%= f.label :comment %> <br>
    <%= f.text_field :comment %>
    <br>
    <%= f.submit %>
  <% end %>
