<%= form_for @bookmark do |f| %>
<p>
<%= f.label :title %><br>
<%= f.text_field :title %>
</p>
<p>
<%= f.label :url %><br>
<%= f.text_field :url %>
</p>
<p>
<%= f.label :comment %><br>
<%= f.text_field :comment %>
</p>
<p>
<%= f.label :category_id %><br>
<%= f.text_field :category_id %>


</p>
<p>
<%= check_box_tag(:favorite) %>
<%= label_tag(:favorite, "Favorite") %>
</p>
<p>
<%= f.submit %>
</p>
<% end %>
