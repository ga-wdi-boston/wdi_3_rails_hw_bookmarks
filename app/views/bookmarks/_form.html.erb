<%= form_for @bookmark do |f| %>
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
    <br>
    <%= f.label :url %> <br>
    <%= f.text_area :url %>
    <br>
    <%= f.label :category %> <br>
    <%= f.text_field :category %>
    <br>
    <%= f.label :comment %> <br>
    <%= f.text_field :comment %>
    <br>
    <%= f.label :favorited? %> <br>
    <%= f.text_field :is_favorited %>
    <br>
<%= f.submit %>
<% end %>
