<%= stylesheet_link_tag "style.css" %>

<%= form_for(@bookmark) do |f| %>

  <div class="field">
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </div>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :comment %><br>
    <%= f.text_field :comment %>
  </div>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
