
<!--NEED TO ADD RADIO BUTTONS FOR CATEGORIES -->

<!--CHECK BOX FAVORITE WORKS BUT NEEDS TO DISPLAY BETTER AND NOT 'TRUE/FALSE' -->

<%= form_for(@bookmark) do |f| %>
  <%= f.text_field :title, placeholder: "Title" %><br>
  <%= f.url_field :url, placeholder: "Paste URL" %><br>
  <%= f.text_area :description, placeholder: "Describe" %><br>
  <%= f.label :favorite %>
  <%= f.check_box :favorite %><br>
<%= f.submit "Submit" %>
<% end %>

