<h1>New Bookmark</h1>

<%= form_for :bookmark do |f| %>
<p>
<%= f.label :title %><br>
<%= f.text_field :title %>
</p>
<p>
<%= f.label :url %><br>
<%= f.text_field :url %>
</p>
<p>
<%= f.label :comment %><br>
<%= f.text_field :comment %>
</p>
<p>
<%= f.label :category %><br>
<%= f.text_field :category %>
</p>
<p>
<%= f.label :favorite %><br>
<%= f.text_field :favorite %>
</p>
<p>
<%= f.submit %>
</p>
<% end %>


