<%= form_for @bookmark do |f| %>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </div>
  <div>
    <%= f.label :comment %><br>
    <%= f.text_area :comment %>
  </div>
  <div>
    <%= f.label :category %><br>
    <%= f.text_field :category %>
  </div>
  <div>
    <%= f.label :is_favorite, 'This one of your favorites?' %><br>
    <%= f.check_box :is_favorite %>
  </div>
  <%= f.submit %>
<% end %>
