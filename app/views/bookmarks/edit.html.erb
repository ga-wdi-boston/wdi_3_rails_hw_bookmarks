<%= form_for(@bookmark, :title => {:action => 'update', :id => @bookmark.id}, :url => {:action => 'update', :id => @bookmark.id}) do |form| %>
  <p><%= form.text_field :title, placeholder: "Enter title" %></p>
  <p><%= form.text_field :url, placeholder: "Enter URL" %></p>
  <p><%= form.text_area :comment, placeholder: "Enter comment" %></p>
  <p>Check here if favorite: <%= form.check_box :favorite%></p>
  <p><%= form.submit %></p>
<% end %>
